{
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[dockerfile]": {
    "breadcrumbs.showBooleans": true,
    "editor.formatOnSave": false,
    "editor.quickSuggestions": {
      "strings": true
    }
  },
  "[go]": {
    "editor.codeActionsOnSave": {
      "source.organizeImports": "explicit"
    }
  },
  "[html]": {
    "editor.defaultFormatter": "vscode.html-language-features"
  },
  "[java]": {
    "breadcrumbs.showArrays": true,
    "editor.defaultFormatter": "redhat.java",
    "editor.suggest.snippetsPreventQuickSuggestions": false
  },
  "[javascript]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "[javascriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[latex]": {
    "editor.formatOnPaste": false,
    "editor.suggestSelection": "recentlyUsedByPrefix",
    "editor.wordWrap": "wordWrapColumn",
    "editor.wordWrapColumn": 160,
    "editor.wrappingIndent": "same"
  },
  "[markdown]": {
    "editor.defaultFormatter": "yzhang.markdown-all-in-one"
  },
  "[python]": {
    "editor.defaultFormatter": "ms-python.black-formatter",
    "editor.formatOnType": true
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.tabSize": 4
  },
  "[vue]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[xml]": {
    "editor.defaultFormatter": "redhat.vscode-xml"
  },
  "[yaml]": {
    "editor.defaultFormatter": "redhat.vscode-yaml"
  },
  "cmake.configureOnOpen": false,
  "cmake.showOptionsMovedNotification": false,
  "debug.showVariableTypes": true,
  "diffEditor.diffAlgorithm": "advanced",
  "diffEditor.experimental.useVersion2": true,
  "diffEditor.hideUnchangedRegions.enabled": true,
  "diffEditor.ignoreTrimWhitespace": false,
  "diffEditor.maxComputationTime": 0,
  "diffEditor.renderSideBySide": true,
  "editor.codeActionsOnSave": {
    "source.addMissingImports": "always",
    "source.fixAll": "always",
    "source.organizeImports": "always"
  },
  "editor.cursorStyle": "line",
  "editor.detectIndentation": false,
  "editor.formatOnSave": true,
  "editor.insertSpaces": true,
  "editor.largeFileOptimizations": false,
  "editor.lineNumbers": "on",
  "editor.minimap.enabled": false,
  "editor.suggestSelection": "first",
  "editor.tabSize": 4,
  "editor.wordSeparators": "/\\()\"':,.;<>~!@#$%^&*|+=[]{}`?-",
  "editor.wordWrap": "off",
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "files.associations": {
    "*.xml": "xml"
  },
  "files.autoSave": "onFocusChange",
  "files.exclude": {
    "**/.classpath": true,
    "**/.factorypath": true,
    "**/.project": true,
    "**/.settings": true
  },
  "git.autofetch": true,
  "git.confirmSync": false,
  "git.enableSmartCommit": true,
  "git.openRepositoryInParentFolders": "never",
  "gitlens.advanced.messages": {
    "suppressLineUncommittedWarning": true
  },
  "gitlens.codeLens.enabled": false,
  "gitlens.currentLine.enabled": false,
  "gitlens.defaultDateFormat": "D MMM YYYY HH:mm",
  "gitlens.defaultDateShortFormat": "D MMM YYYY",
  "gitlens.defaultTimeFormat": "HH:mm",
  "gitlens.graph.avatars": false,
  "gitlens.graph.dateFormat": null,
  "gitlens.graph.dimMergeCommits": true,
  "gitlens.graph.minimap.additionalTypes": ["localBranches"],
  "gitlens.hovers.currentLine.over": "line",
  "gitlens.remotes": [
    {
      "domain": "git.inventage.com",
      "type": "BitbucketServer"
    }
  ],
  "gitlens.statusBar.pullRequests.enabled": false,
  "go.formatTool": "goimports",
  "go.toolsManagement.autoUpdate": true,
  "go.useLanguageServer": true,
  "[helm]": {
    "editor.formatOnSave": false
  },
  "intellicodeApiExamples.typescript.enabled": false,
  "java.cleanup.actionsOnSave": ["addOverride"],
  "java.codeGeneration.generateComments": true,
  "java.compile.nullAnalysis.mode": "automatic",
  "java.completion.chain.enabled": true,
  "java.completion.importOrder": ["#", ""],
  "java.configuration.updateBuildConfiguration": "disabled",
  "java.eclipse.downloadSources": true,
  "java.help.firstView": "none",
  "java.inlayHints.parameterNames.enabled": "none",
  "java.jdt.ls.androidSupport.enabled": "off",
  "java.maven.downloadSources": true,
  "java.project.importOnFirstTimeStartup": "automatic",
  "java.saveActions.organizeImports": true,
  "latex-workshop.latex.autoBuild.run": "never",
  "latex-workshop.view.pdf.viewer": "browser",
  "markdown.updateLinksOnFileMove.enabled": "always",
  "notebook.cellToolbarLocation": {
    "default": "right",
    "jupyter-notebook": "left"
  },
  "prettier.ignorePath": "~/.prettierignore",
  "protoc": {
    "clang-format.style": "{ IndentWidth: 4, BasedOnStyle: google, AlignConsecutiveAssignments: true }"
  },
  "python.formatting.provider": "black",
  "python.languageServer": "Pylance",
  "python.showStartPage": false,
  "python.terminal.activateEnvironment": true,
  "redhat.telemetry.enabled": false,
  "scm.experimental.showHistoryGraph": true,
  "security.workspace.trust.untrustedFiles": "open",
  "terminal.integrated.enableMultiLinePasteWarning": false,
  "terminal.integrated.enablePersistentSessions": false,
  "terminal.integrated.persistentSessionReviveProcess": "never",
  "vim.enableNeovim": true,
  "vim.handleKeys": {
    "<C-p>": false
  },
  "vim.neovimPath": "/usr/bin/nvim",
  "vim.textwidth": 160,
  "vs-kubernetes": {
    "vs-kubernetes.crd-code-completion": "disabled",
    "vs-kubernetes.minikube-show-information-expiration": "2024-04-14T14:07:29.675Z",
    "vscode-kubernetes.minikube-path.linux": "~/.vs-kubernetes/tools/minikube/linux-amd64/minikube"
  },
  "window.zoomLevel": -1,
  "workbench.colorTheme": "Default Dark+",
  "workbench.editor.empty.hint": "hidden",
  "workbench.editorAssociations": {
    "*.ipynb": "jupyter-notebook",
    "git-rebase-todo": "gitlens.rebase"
  },
  "workbench.panel.defaultLocation": "right",
  "workbench.startupEditor": "newUntitledFile",
  "xmlTools.splitXmlnsOnFormat": false,
  "yaml.format.bracketSpacing": false,
  "chat.commandCenter.enabled": false,
  "yaml.schemas": {
    "~/git/inventage/ips/portal-helm-chart-library/helm/src/main/resources/values.schema.json": [
      "~/git/inventage/ips/*/helm/src/main/resources/**/values*.yaml"
    ]
  },
  "json.schemas": [
    {
      "url": "~/git/inventage/ips/portal-gateway/config-schemas/src/main/resources/schema/portalGatewayDynamicSchema.json",
      "fileMatch": [
        "~/git/inventage/ips/portal-gateway/helm/src/main/resources/**/dynamic-config/**/config.json"
      ]
    },
    {
      "url": "~/git/inventage/ips/portal-gateway/config-schemas/src/main/resources/schema/portalGatewayStaticSchema.json",
      "fileMatch": [
        "~/git/inventage/ips/portal-gateway/helm/src/main/resources/**/portal-gateway.json"
      ]
    }
  ],
  "editor.accessibilitySupport": "off"
}
