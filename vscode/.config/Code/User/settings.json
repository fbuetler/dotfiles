{
  "C_Cpp.updateChannel": "Insiders",
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[dockerfile]": {
    "breadcrumbs.showBooleans": true,
    "editor.formatOnSave": false,
    "editor.quickSuggestions": {
      "strings": true
    }
  },
  "[go]": {
    "editor.codeActionsOnSave": {
      "source.organizeImports": true
    }
  },
  "[html]": {
    "editor.defaultFormatter": "vscode.html-language-features"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[latex]": {
    "editor.formatOnPaste": false,
    "editor.suggestSelection": "recentlyUsedByPrefix",
    "editor.wordWrap": "wordWrapColumn",
    "editor.wordWrapColumn": 80,
    "editor.wrappingIndent": "same"
  },
  "[markdown]": {
    "editor.defaultFormatter": "yzhang.markdown-all-in-one"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.tabSize": 2
  },
  "[vue]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[yaml]": {
    "editor.defaultFormatter": "redhat.vscode-yaml"
  },
  "cmake.configureOnOpen": false,
  "diffEditor.ignoreTrimWhitespace": false,
  "diffEditor.maxComputationTime": 0,
  "editor.codeActionsOnSave": {
    "source.organizeImports": true
  },
  "editor.cursorStyle": "line",
  "editor.formatOnSave": true,
  "editor.insertSpaces": false,
  "editor.largeFileOptimizations": false,
  "editor.lineNumbers": "on",
  "editor.minimap.enabled": false,
  "editor.suggestSelection": "first",
  "editor.tabSize": 8,
  "editor.wordSeparators": "/\\()\"':,.;<>~!@#$%^&*|+=[]{}`?-",
  "editor.wordWrap": "off",
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "files.autoSave": "onFocusChange",
  "files.exclude": {
    "**/.classpath": true,
    "**/.factorypath": true,
    "**/.project": true,
    "**/.settings": true
  },
  "git.autofetch": true,
  "git.confirmSync": false,
  "git.enableSmartCommit": true,
  "gitlens.advanced.messages": {
    "suppressLineUncommittedWarning": true
  },
  "gitlens.codeLens.enabled": false,
  "gitlens.currentLine.enabled": false,
  "gitlens.defaultDateFormat": "D MMM YYYY HH:mm",
  "gitlens.defaultDateShortFormat": "D MMM YYYY",
  "gitlens.defaultTimeFormat": "HH:mm",
  "gitlens.hovers.currentLine.over": "line",
  "gitlens.remotes": [
    {
      "domain": "git.inventage.com",
      "type": "BitbucketServer"
    }
  ],
  "go.autocompleteUnimportedPackages": true,
  "go.formatTool": "goimports",
  "go.toolsManagement.autoUpdate": true,
  "go.useCodeSnippetsOnFunctionSuggest": false,
  "go.useLanguageServer": true,
  "latex-workshop.view.pdf.viewer": "browser",
  "java.configuration.updateBuildConfiguration": "disabled",
  "java.format.comments.enabled": false,
  "java.inlayHints.parameterNames.enabled": "none",
  "java.project.importOnFirstTimeStartup": "automatic",
  "notebook.cellToolbarLocation": {
    "default": "right",
    "jupyter-notebook": "left"
  },
  "prettier.ignorePath": "~/.prettierignore",
  "python.formatting.provider": "black",
  "python.languageServer": "Pylance",
  "python.showStartPage": false,
  "python.terminal.activateEnvironment": false,
  "redhat.telemetry.enabled": false,
  "security.workspace.trust.untrustedFiles": "open",
  "terminal.integrated.persistentSessionReviveProcess": "never",
  "terminal.integrated.enableMultiLinePasteWarning": false,
  "terminal.integrated.enablePersistentSessions": false,
  "vim.enableNeovim": true,
  "vim.neovimPath": "/usr/bin/nvim",
  "vim.textwidth": 80,
  "vs-kubernetes": {
    "vscode-kubernetes.minikube-path.linux": "~/.vs-kubernetes/tools/minikube/linux-amd64/minikube"
  },
  "window.zoomLevel": 1,
  "workbench.editor.untitled.hint": "hidden",
  "workbench.editorAssociations": {
    "*.ipynb": "jupyter-notebook"
  },
  "workbench.panel.defaultLocation": "bottom",
  "workbench.startupEditor": "newUntitledFile",
  "yaml.format.bracketSpacing": false
}
